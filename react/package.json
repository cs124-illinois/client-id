    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "npm-check-updates": "17.1.0",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.0.0",
    "react": "18.3.1",
