{
  "private": true,
  "name": "demo",
  "version": "2023.5.0",
  "scripts": {
    "build": "next build",
    "checker": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs .next,.rush,bundled --ignores=eslint-*,prettier-plugin-*",
    "eslint": "eslint --fix pages --max-warnings=0 --ext .ts,.tsx",
    "lint": "next lint",
    "ncu": "ncu",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "serve": "next start",
    "start": "next dev",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@cs124/client-id": "2024.8.0",
    "next": "14.2.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "22.5.0",
    "@types/react": "18.3.4",
    "depcheck": "1.4.7",
    "eslint": "9.9.0",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "npm-check-updates": "17.1.0",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.0.0",
    "sass": "1.77.8",
    "typescript": "5.5.4"
  }
}
