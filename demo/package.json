{
  "private": true,
  "name": "demo",
  "version": "2023.5.0",
  "scripts": {
    "build": "next build",
    "checker": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs .next,.rush,bundled --ignores=eslint-*,prettier-plugin-*",
    "eslint": "eslint --fix pages --max-warnings=0 --ext .ts,.tsx",
    "lint": "next lint",
    "ncu": "ncu",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "serve": "next start",
    "start": "next dev",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@cs124/client-id": "2024.8.1",
    "next": "15.1.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "22.10.6",
    "@types/react": "19.0.6",
    "depcheck": "1.4.7",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "npm-check-updates": "17.1.13",
    "prettier": "3.4.2",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "sass": "1.83.1",
    "typescript": "5.7.3"
  }
}
